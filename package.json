{
  "name": "littlebiglab",
  "version": "1.1.0",
  "author": "Nathan Lenkowski <nathan@littlebiglab.com>",
  "homepage": "https://littlebiglab.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/nlenkowski/littlebiglab"
  },
  "bugs": {
    "url": "https://github.com/nlenkowski/littlebiglab/issues"
  },
  "license": "MIT",
  "browserslist": [
    ">0.25%"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm run development",
    "development": "npm run clean && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "npm run clean && cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch"
  },
  "devDependencies": {
    "@ayctor/laravel-mix-svg-sprite": "github:ayctor/laravel-mix-svg-sprite#next",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "laravel-mix": "^5.0.1",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^3.0.2",
    "sass": "^1.25.0",
    "sass-loader": "8.*",
    "stylelint": "^13.2.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.14.2",
    "svg-spritemap-webpack-plugin": "^3.5.5",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "include-media": "^1.4.9",
    "nodelist-foreach-polyfill": "^1.2.0",
    "normalize.css": "^8.0.1",
    "snapsvg-cjs": "0.0.6",
    "svgxuse": "^1.2.6"
  }
}
